{
  "name": "citibike-station-data",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22",
    "pnpm": "please-use-npm",
    "yarn": "please-use-npm"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "seed": "npx prisma db seed",
    "update": "npm run download && npm run seed",
    "download": "node scripts/downloader.mjs",
    "postinstall": "prisma generate",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs,.mts",
    "prepare": "husky",
    "cypress": "cypress open",
    "cypress:cli": "cypress run"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "**/*.jsx": [
      "eslint",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/x-data-grid": "^8.4.0",
    "@mui/x-date-pickers": "^7.7.1",
    "@prisma/client": "^6.8.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@vercel/analytics": "^1.5.0",
    "adm-zip": "^0.5.14",
    "autosuggest-highlight": "^3.3.4",
    "concat-files": "^0.1.1",
    "date-fns": "^4.1.0",
    "immer": "^10.1.1",
    "next": "^15.3.2",
    "nuqs": "^2.4.3",
    "prisma-repl": "^1.4.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "recharts": "^2.12.7",
    "redis": "^5.1.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.3.2",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-js": "^4.4.0",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/cypress": "^0.1.6",
    "@types/node": "^22.15.24",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.32.0",
    "ascii-progress": "^2.0.0",
    "autoprefixer": "^10.4.19",
    "babel-plugin-react-compiler": "^19.0.0-beta-714736e-20250131",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.0",
    "cypress": "^14.4.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "postcss": "^8.4.38",
    "prettier": "npm:@btmills/prettier@^3.2.5",
    "prisma": "^6.8.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2"
  }
