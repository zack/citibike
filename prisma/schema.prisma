generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Dock {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model Trip {
  id            Int    @id @default(autoincrement())
  day           Int
  month         Int
  year          Int
  startDockName String
  endDockName   String

  @@index([endDockName], type: Hash)
  @@index([startDockName], type: Hash)
}
