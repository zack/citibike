generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Dock {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model Trip {
  id            Int      @id @default(autoincrement())
  startDockName String
  endDockName   String
  startedAt     DateTime
  endedAt       DateTime

  @@index([endDockName], type: Hash)
  @@index([startDockName], type: Hash)
}
